{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
      "baseUrl": ".",
      "paths": {
        "*": ["src/*"],
        "@components/*": ["src/components/*"],
        "@screens/*": ["src/screens/*"],
        "@constants/*": ["src/constants/*"],
        "@contexts/*": ["src/contexts/*"],
        "@hooks/*": ["src/hooks/*"],
        "@store/*": ["src/store/*"],
        "@utils/*": ["src/utils/*"],
        "@types/*": ["src/types/*"],
        "@navigator/*": ["src/navigator/*"],
        "@services/*": ["src/services/*"],
      },
    }
}
